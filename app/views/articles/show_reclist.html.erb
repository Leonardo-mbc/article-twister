<% @articles.each do |key, news| %>
    [<%= key %>]<br/>
    <table class="table table-hover">
        <% news.each do |art| %>
            <tr class="<%=tr_sign art.sign(current_user.id) %>">
                <td><%= art.title %>, <%= art.source %></td>
            </tr>
        <% end %>
    </table>
<% end %>
